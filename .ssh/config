Host *
  VisualHostKey yes
  ControlPath ~/.ssh/%r@%h.sock
  ControlPersist 5m

Host jupiter.firemirror.com
  ForwardAgent yes
  ControlMaster auto

Host *.scsys.co.uk
  User ashb

Host hack.rs
  User ashb

Host *.socialflow.com
  ForwardAgent no
  ControlMaster auto

Host dev8.socialflow.com
  LocalForward  5069 127.0.0.1:4005

