ServerAliveCountMax 6
ConnectTimeout 60
#ProxyCommand /usr/bin/nc -X5 -x192.168.0.1:8080 %h %p

Host *
  VisualHostKey yes
  ControlPath ~/.ssh/%r@%h.sock
  ControlPersist 5m

Host jupiter.firemirror.com
  ForwardAgent yes
  ControlMaster auto

Host callisto.firemirror.com
  User ash

Host *.jupiter.firemirror.com
  ControlMaster auto
  ProxyCommand ssh -o VisualHostKey=no jupiter.firemirror.com nc -q0 %h %p 2>/dev/null

